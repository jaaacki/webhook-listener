# =============================================================================
# Webhook Listener Configuration
# =============================================================================
# Copy this file to .env and adjust values as needed.
# =============================================================================

# Comma-separated list of webhook namespaces (endpoints)
# Each namespace creates an endpoint at /hook/<namespace>
NAMESPACES=alpha,beta,gamma,delta,epsilon,zeta,eta,theta,iota,kappa

# Maximum number of events to keep in memory per namespace (oldest are evicted)
MAX_EVENTS_PER_NAMESPACE=1000
